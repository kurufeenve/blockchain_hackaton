  <script>
function block() {
          console.log("Starts");
          var url = "http://46.101.135.143:3000/find/";
          var cont = document.getElementById("output");
	cont.innerHTML = '';
          var num = document.getElementById("inp_carnum").value;
          url = url + num;
		var arr = [
					"abc", "Name ", "Sertificate num ", "From ", "To ", "Link to documents"
		];
      $.get(url, function(data){
            var jsonObject = JSON.parse(data);
		
            for (var x in jsonObject) {
		if (x == 0){
			continue;
		}
		if (jsonObject[x] == '') {
			cont.innerHTML = "Certificate not found";
			break;
		}
                var div = document.createElement('div');
                div.innerHTML = arr[x] + "   :   " + jsonObject[x];
                cont.appendChild(div);
            }
            // alert("Data: " + data);
            // alert("JSON: " + jsonObject);
          });
        }
</script>
